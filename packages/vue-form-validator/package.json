{
  "name": "@convidera-team/vue-form-validator",
  "version": "1.0.1",
  "files": [
    "dist"
  ],
  "module": "./dist/lib.es.js",
  "exports": {
    ".": {
      "import": "./dist/lib.es.js"
    }
  },
  "typings": "./dist/lib.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "typecheck": "vue-tsc --noEmit",
    "typewatch": "vue-tsc --noEmit --watch",
    "lint": "yarn lint:js",
    "lint:fix": "yarn lint:js:fix",
    "lint:js": "eslint --ext .js,.vue,.ts --ignore-path .gitignore .",
    "lint:js:fix": "yarn lint:js --fix",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "devDependencies": {
    "@convidera-team/eslint-config-vue3-convidera": "^1.2.1",
    "@convidera-team/utils": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "@vitejs/plugin-vue": "^4.1.0",
    "axios": "^1.3.5",
    "eslint": "^8.38.0",
    "typescript": "^5.0.4",
    "vite": "^4.2.1",
    "vite-plugin-dts": "^2.2.0",
    "vitest": "^0.30.1",
    "vue": "^3.2.47",
    "vue-tsc": "^1.3.4"
  },
  "peerDependencies": {
    "axios": ">=0.2",
    "vue": ">=3"
  },
  "publishConfig": {
    "access": "public"
  }
}
