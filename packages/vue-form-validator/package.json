{
  "name": "@convidera-team/vue-form-validator",
  "version": "1.1.5",
  "repository": {
    "type": "git",
    "url": "https://github.com/convidera/frontend-kit.git",
    "directory": "packages/vue-form-validator"
  },
  "files": [
    "dist"
  ],
  "module": "./dist/lib.es.js",
  "exports": {
    ".": {
      "import": "./dist/lib.es.js",
      "types": "./dist/lib.d.ts"
    }
  },
  "typings": "./dist/lib.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "typecheck": "vue-tsc --noEmit",
    "typewatch": "vue-tsc --noEmit --watch",
    "lint": "yarn lint:js",
    "lint:fix": "yarn lint:js:fix",
    "lint:js": "eslint --ext .js,.vue,.ts --ignore-path .gitignore .",
    "lint:js:fix": "yarn lint:js --fix",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "devDependencies": {
    "@convidera-team/eslint-config-vue3-convidera": "^2.0.1",
    "@convidera-team/utils": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "@vitejs/plugin-vue": "^6.0.0",
    "axios": "^1.10.0",
    "eslint": "^9.29.0",
    "typescript": "^5.8.3",
    "vite": "^7.0.0",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4",
    "vue": "^3.5.17",
    "vue-tsc": "^2.2.10"
  },
  "peerDependencies": {
    "axios": ">=0.2",
    "vue": ">=3"
  },
  "publishConfig": {
    "access": "public"
  }
